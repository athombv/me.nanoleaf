{
  "id": "me.nanoleaf",
  "sdk": 2,
  "brandColor": "#3FAE49",
  "name": {
    "en": "Nanoleaf"
  },
  "description": {
    "en": "Adds support for Nanoleaf Aurora devices"
  },
  "category": "lights",
  "version": "2.0.2",
  "compatibility": ">=2.1.1",
  "author": {
    "name": "Athom B.V."
  },
  "images": {
    "large": "/assets/images/large.jpg",
    "small": "/assets/images/small.jpg"
  },
  "discovery": {
    
    "nanoleaf-aurora": {
      "type": "mdns-sd",
      "mdns": {
        "name": "nanoleafapi",
        "protocol": "tcp"
      },
      "id": "{{txt.id}}",
      "conditions": [
        [
          {
            "field": "txt.md",
            "match": {
              "type": "string",
              "value": "NL22"
            }
          }
        ]
      ]
    },
    
    "nanoleaf-canvas": {
      "type": "mdns-sd",
      "mdns": {
        "name": "nanoleafapi",
        "protocol": "tcp"
      },
      "id": "{{txt.id}}",
      "conditions": [
        [
          {
            "field": "txt.md",
            "match": {
              "type": "string",
              "value": "NL29"
            }
          }
        ]
      ]
    }
  }
  
}